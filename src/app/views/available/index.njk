{%for hour in available%}
  <label class="{% if hour.available === false%}disabled{% endif %}"for="time-{{ hour.timer }}">
    {% if hour.available === true%}
      <input type="radio" name="date" value="{{ hour.value }}" id="time-{{ hour.timer }}"/>
    {% endif %}
    {{ hour.time }}
  </label>
{% endfor %}
